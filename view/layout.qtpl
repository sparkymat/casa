{% func Layout(title string, content string) %}
  <!DOCTYPE html>
  <html>
    <head>
      <meta charset="utf-8">
      <meta name="viewport" content="width=device-width,initial-scale=1">
      <title>{%s title %}</title>
      <link rel="stylesheet" type="text/css" href="/css/style.css">
    </head>
    <body>
      {%s= content %}
      <script src="/javascript/index.js"></script>
    </body>
  </html>
{% endfunc %}

